  "private": true,
  "scripts": {
    "android": "APP_ENV=development react-native run-android",
    "android:prod": "APP_ENV=production react-native run-android",
    "android:stage": "APP_ENV=staging react-native run-android",
    "android:dev": "APP_ENV=development react-native run-android",
    "start:staging": "APP_ENV=staging npx react-native start --reset-cache",
    "start:prod": "APP_ENV=production npx react-native start --reset-cache",
    "start:dev": "APP_ENV=development npx react-native start --reset-cache",
    "android:clean": "cd ./android && ./gradlew clean && cd ..",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint:rules": "eslint . --cache",
    "lint:code-format": "prettier --check \"{src,__mocks__}/**/*.{js,json,md,ts,tsx,yml,yaml}\"",
    "lint:type-check": "tsc",
    "lint": "yarn lint:rules && yarn lint:code-format && yarn lint:type-check",
    "lint:fix": "yarn lint:rules --fix && yarn lint:code-format --write && yarn lint:type-check",
    "test:report": "jest --collectCoverage --coverageDirectory=\"./coverage\" --ci --reporters=default --reporters=jest-junit --coverage",
    "pod-install": "npx pod-install"
